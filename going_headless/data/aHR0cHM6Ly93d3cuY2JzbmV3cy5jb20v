<section class="component component--topic-cbsn component--type-filtered_listing component--view-video-shelf-with-hero-has-draggable-limit-5" id="component-top-stories" data-id="9010183f-9343-4abb-b707-46dbecd783fe" data-slug="top-stories"><h3 class="component__title">Top Stories</h3>
  
  
  <div class="component__bg"></div>

  <div class="component__item-wrapper">

                                      
        



<article class="item item--type-cbsn_live item--topic-cbsn"><a href="https://www.cbsnews.com/live/" class="item__anchor" draggable="false" data-invalid-url-rewritten-http="">

    <div class="item__icon item__icon--cbsn_live">
      <svg viewbox="0 0 32 32" class="icon__overlay--cbsn_live" aria-hidden="true"></svg></div>

                  
        <div class="item__video">
          

<link rel="preload" as="script" href="https://cbsnews4.cbsistatic.com/fly/bundles/cbsnewscore/js-build/main.video-embed.js?v=1.750d7cf740c4de27817b8a735590e20a806c922d"><svg xmlns="http://www.w3.org/2000/svg" style="position:absolute;width:0;height:0"></svg><div class="embed__content--draggable" data-component="draggableFrame" data-draggable-frame-options='{"pauseOnClose":true,"appearAfterClose":true}'>

    <div class="player-overlay__container player-overlay--draggable uvpjs" data-theme="default">
      <div class="player-overlay">
        <svg class="player-overlay--icon-grid"></svg><span class="player-overlay__title" data-component="cbsnLiveHeadline"> CBSN </span>
                
