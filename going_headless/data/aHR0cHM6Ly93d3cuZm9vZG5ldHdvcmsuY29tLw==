    <div class="m-MediaBlock m-Slide__m-MediaBlock">
    
      
      <div class="m-MediaBlock__m-MediaWrap">
            
  <a href="#" title="Giant Reese's Cup with Pieces">
    <img class="m-MediaBlock__a-Image a-Image" src="//food.fnr.sndimg.com/content/dam/images/food/video/0/02/029/0298/0298743.jpg.rend.hgtvcom.196.110.suffix/1521667126075.jpeg">
    
    
    
